<fieldset data-payment-target="additionalFields" data-type="Check">
  <div class="my-5">
    <%= form.label t('.routing_number') %>
    <%= form.text_field :routing_number, class: "block shadow rounded-md border border-gray-200 outline-none px-3 py-2 mt-2 w-full" %>
  </div>
  
  <div class="my-5">
    <%= form.label t('.account_number') %>
    <%= form.password_field :account_number, class: "block shadow rounded-md border border-gray-200 outline-none px-3 py-2 mt-2 w-full" %>
  </div>
</fieldset>
